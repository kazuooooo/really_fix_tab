{
    "name": "Really Fix Tab",
    "version": "0.1",
    "manifest_version": 2,

    "description": "Never Jump Other Page On FixedTab",
    "permissions": [
        "tabs"
    ],

    "background": {
    "scripts": ["tab.js"]
    },

    "content_scripts": [
        {
            "matches": ["http://*/*","https://*/*"],
            "js": ["jquery.min.js","background.js"]
        }
    ]
}
